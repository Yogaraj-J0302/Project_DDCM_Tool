@code{
import java.text.DecimalFormat;
function fnFormat(strPattern,dAmount)
{
    return new DecimalFormat(strPattern).format(dAmount);
}
function fnFormatIndAmt(strTotAmt)
{
if(strTotAmt != '')
{
Boolean bNegative = false;
String strvalue="";

Double value = Double.valueOf(strTotAmt);
if(value < 0)
{
    value=value*-1;
    bNegative = true;
}
if(value < 1000) {
                strvalue = fnFormat("##0.00", value);
            } 
else {
                double hundreds = value % 1000;
                int other = (int) (value / 1000);
                strvalue = fnFormat(",##", other) + ',' + fnFormat("000.00", hundreds);
        }

if(bNegative)
    strvalue = "-"+strvalue;
  
return strvalue;
}
else
{
   return "0.00";
}
}
function fntokenextract(text)
{
if (text!='')
{
      String token = text.substring(1,text.indexOf('>'));
      String token_ed = text.substring(text.lastIndexOf('/')+1,text.lastIndexOf('>'));
if(token==token_ed)
{
      String str = text.substring(('<'+token+'>').length(),text.lastIndexOf('</'));
    return str;
}
else
{
return text;
}
}
else
{
return text;
}
}
 }
